{
	"compilerOptions": {
		"jsx": "react-jsx",
		"moduleResolution": "Bundler",
		"module": "ESNext",
		"target": "ES2022",
		"skipLibCheck": true,
		"strictNullChecks": true,
		"baseUrl": ".",
		"paths": {
			"@/*": ["./app/*"]
		},
		"forceConsistentCasingInFileNames": true
	}
	// "include": ["app", "components", "lib", "types"]
}
